<h1>Site Text</h1>
<br>
<div id="pagecontent">
<div class="top-left"></div><div class="top-right"></div>
	<div class="inside">
		<p class="notopgap">&nbsp;</p>
		<!-- Start Of Content -->
		<p>
		  <b>Identifier:</b>
		  <%=h @sitetext.textkey %>
		</p>

		<p>
		  <u><b>The Text:</b></u><br>
		  <%=h @sitetext.sitetext %>
		</p>

		<%= link_to 'Edit', edit_sitetext_path(@sitetext) %> |
		<%= link_to 'Back To Sitetext List', sitetexts_path %>
		<br>
		<!-- End Of Content -->
		<br>
		<p class="nobottomgap"></p>
	</div>
<div class="bottom-left"></div><div class="bottom-right"></div>
</div> <!-- End Page Content -->
		